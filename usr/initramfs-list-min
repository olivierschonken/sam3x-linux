# This is a very simple, default initramfs
dir /tmp 0755 0 0
dir /dev 0755 0 0
nod /dev/console 0600 0 0 c 5 1
nod /dev/null 0600 0 0 c 1 3

dir /root 0700 0 0
dir /mnt 0755 0 0
dir /proc 0755 0 0
dir /sys 0755 0 0
dir /etc 0755 0 0
dir /bin 755 0 0
dir /sbin 755 0 0
file /init usr/init 777 0 0
file /bin/busybox usr/busybox 755 0 0 
slink /bin/hush busybox 777 0 0 
dir /usr 755 0 0
dir /usr/bin 755 0 0
dir /usr/sbin 755 0 0

